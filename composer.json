{
    "name": "kksoftwareag/session-popup",
    "type": "typo3-cms-extension",
    "description": "Configure a popup that appears once a session. based on \"webschmiede/session-popup ",
    "version": "4.0.0",
    "authors": [
        {
            "name": "Karina Kern",
            "role": "Developer"
        },
        {
            "name": "Thomas Paul",
            "role": "Developer"
        },
        {
          "name": "Bernhard Sirlinger",
          "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": ">= 12.4"
    },
    "require-dev": {
        "typo3/testing-framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Webschmiede\\SessionPopup\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webschmiede\\SessionPopup\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/session-popup": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "session_popup"
        }
    }
}
